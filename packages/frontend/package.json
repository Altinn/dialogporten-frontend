    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "dialogporten-types-generated": "workspace:*",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "i18next": "^23.8.2",
    "i18next-icu": "^2.3.0",
    "leva": "^0.9.35",
