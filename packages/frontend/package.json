{
  "name": "frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./index.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest run",
    "codegen:playwright": "playwright codegen",
    "test:playwright": "npx playwright test --project=playwright",
    "test:playwright:mobile": "npx playwright test --project=playwright-mobile",
    "test:accessibility": "npx playwright test --project=accessibility",
    "install:browsers": "playwright install",
    "test:watch": "vitest watch",
    "build:docker": "docker build -f ./Dockerfile -t frontend ../..",
    "run:docker": "docker run -it -p 80:80 frontend",
    "i18n:check": "tsx ./src/i18n/check.ts",
    "i18n:sort": "tsx ./src/i18n/check.ts --sort"
  },
  "dependencies": {
    "@altinn/altinn-components": "0.54.2",
    "@microsoft/applicationinsights-react-js": "19.3.7",
    "@microsoft/applicationinsights-web": "3.3.9",
    "@navikt/aksel-icons": "7.38.0",
    "@reactour/tour": "3.8.0",
    "@tanstack/react-query": "5.90.16",
    "bff-types-generated": "workspace:*",
    "classnames": "2.5.1",
    "date-fns": "3.6.0",
    "embeddable-markdown-html": "workspace:*",
    "graphql": "16.12.0",
    "graphql-request": "7.4.0",
    "i18next": "23.16.8",
    "i18next-icu": "2.4.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-i18next": "15.7.4",
    "react-router-dom": "7.12.0",
    "sse.js": "2.7.2",
    "use-debounce": "10.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@playwright/test": "1.57.0",
    "@testing-library/react": "16.3.1",
    "@types/node": "22.19.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/reactour": "1.18.5",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react-swc": "3.11.0",
    "axe-html-reporter": "2.2.11",
    "dotenv": "16.6.1",
    "jsdom": "26.1.0",
    "msw": "2.12.7",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "3.2.4",
    "web-streams-polyfill": "3.3.3"
  },
  "msw": {
    "workerDirectory": [
      "public",
      "dist"
    ]
  }
}
