{
  "name": "bff",
  "main": "src/index.ts",
	"private": true,
	"scripts": {
		"start": "ts-node src/index.ts",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeormold": "typeorm-ts-node-commonjs -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/data-source.ts",
    "typeorm:local": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/data-source.local.ts",
    "typeorm:migrate": "npm run typeorm migration:generate -- -n",
    "migration:create": "node src/scripts/migration-create.js",
    "typeorm:run": "typeorm-ts-node-commonjs migration:run -- -d src/data-source.ts",
    "typeorm:revert": "npm run typeorm migration:revert",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon --watch 'src/**/*.ts'\"",
    "build:docker": "docker build -f ./Dockerfile -t bff ../..",
    "run:docker": "docker run -it -p 80:80 bff"
  },
  "dependencies": {
    "@azure/app-configuration": "^1.4.1",
    "@azure/identity": "^4.0.0",
    "@azure/keyvault-keys": "^4.7.2",
    "@types/body-parser": "^1.19.5",
    "applicationinsights": "^2.9.0",
    "axios": "^1.6.2",
    "azure-app-configuration": "^0.0.1-security.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "openid-client": "^5.6.1",
    "passport": "^0.6.0",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "swagger-autogen": "^2.23.5",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.19",
    "util": "^0.12.5",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@azure/keyvault-secrets": "^4.7.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/express-session": "^1.17.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^16.11.10",
    "@types/passport": "^1.0.16",
    "@types/reflect-metadata": "^0.1.0",
    "@types/swagger-ui-express": "^4.1.3",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.2",
    "typescript": "4.5.2"
  }
}
