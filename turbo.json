{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:bff-types-generated": {
      "outputs": ["generated/**"],
      "cache": false,
      "dependsOn": ["build:bff"]
    },
    "build:@digdir/dialogporten-node-logger": {
      "outputs": ["packages/node-logger/dist/**"],
      "cache": false
    },
    "build": {
      "env": ["BASEURL", "URL"],
      "cache": false,
      "dependsOn": ["build:@digdir/dialogporten-node-logger"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["build:@digdir/dialogporten-node-logger"]
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "outputs": [],
      "cache": false
    },
    "build:docker": {
      "outputs": [],
      "cache": false
    },
    "build:frontend": {
      "cache": false,
      "dependsOn": ["build:bff-types-generated"]
    },
    "build:bff": {
      "outputs": [],
      "cache": false,
      "dependsOn": ["build:@digdir/dialogporten-node-logger"]
    }
  }
}
