{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:bff-types-generated": {
      "dependsOn": ["^build"],
      "outputs": ["generated/**"],
      "cache": false
    },
    "build": {
      "dependsOn": ["build:bff-types-generated"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "outputs": [],
      "cache": false
    },
    "build:docker": {
      "outputs": [],
      "cache": false
    }
  }
}
