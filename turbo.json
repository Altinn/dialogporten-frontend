{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:@digdir/dialogporten-node-logger": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "packages/node-logger/dist/**"
      ],
      "cache": false
    },
    "build:bff-types-generated": {
      "dependsOn": [
        "build:@digdir/dialogporten-node-logger",
        "^build"
      ],
      "outputs": [
        "generated/**"
      ],
      "cache": false
    },
    "build": {
      "env": [
        "BASEURL",
        "URL",
        "BUILD_VERSION"
      ],
      "dependsOn": [
        "build:bff-types-generated"
      ],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "typecheck": {
      "outputs": [],
      "cache": false
    },
    "build:docker": {
      "outputs": [],
      "cache": false
    }
  }
}